<div class="row question-container-printanswers">
    {% block question_name %}
    <div class="col-md-4">
        <b>{{question.question}} ({{question.qid}})</b><br/>
        <small>{{gT("Type:")}} ({{question.questionclass}})</small>
    </div>
    {% endblock  %}
    {% block question_answer %}
        {{ include('./subviews/printanswers/question_types/template_generic_question.twig') }}
    {% endblock  %}
</div>

