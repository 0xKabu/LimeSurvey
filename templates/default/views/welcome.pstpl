<div class="alert alert-danger" role="alert"  id="checkjavascript">
    {CHECKJAVASCRIPT}
</div>

<script>
  document.getElementById("checkjavascript").style.display = 'none';
</script>

<!-- Survey Description -->
<div class="row" id="welcome-container">

